{
    "name": "@cloudflare/actors",
    "version": "0.0.1-beta.6",
    "description": "An easier way to build with Cloudflare Durable Objects",
    "type": "module",
    "main": "./dist/core/index.js",
    "types": "./dist/core/index.d.ts",
    "workspaces": [
        "packages/*",
        "examples/*"
    ],
    "exports": {
        ".": {
            "types": "./dist/core/index.d.ts",
            "workerd": "./dist/core/index.js",
            "import": "./dist/core/index.js",
            "default": "./dist/core/index.js"
        },
        "./storage": {
            "types": "./dist/storage/index.d.ts",
            "workerd": "./dist/storage/index.js",
            "import": "./dist/storage/index.js",
            "default": "./dist/storage/index.js"
        },
        "./alarms": {
            "types": "./dist/alarms/index.d.ts",
            "workerd": "./dist/alarms/index.js",
            "import": "./dist/alarms/index.js",
            "default": "./dist/alarms/index.js"
        },
        "./sockets": {
            "types": "./dist/sockets/index.d.ts",
            "workerd": "./dist/sockets/index.js",
            "import": "./dist/sockets/index.js",
            "default": "./dist/sockets/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "deploy": "wrangler deploy",
        "cf-typegen": "wrangler types",
        "build": "tsdown",
        "dev": "vite",
        "clean": "rm -rf dist .wrangler worker-configuration.d.ts"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "cloudflare",
        "workers",
        "actor",
        "framework",
        "distributed-systems"
    ],
    "author": "Cloudflare",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cloudflare/actors"
    },
    "optionalDependencies": {
        "cron-schedule": "^5.0.4",
        "nanoid": "^5.1.5"
    },
    "devDependencies": {
        "@cloudflare/vite-plugin": "1.12.3",
        "@cloudflare/workers-types": "^4.20251213.0",
        "tsdown": "^0.18.0",
        "typescript": "^5.5.2",
        "vite": "^6.3.5",
        "wrangler": "^4.34.0"
    }
}
