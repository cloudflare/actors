{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"outDir": "../dist",
		"rootDir": ".",
		"composite": true,
		"types": ["@cloudflare/workers-types"]
	},
	"exclude": ["node_modules", "dist"]
}
